/* General Styles */
body {
    font-family: Arial, sans-serif;
    background-color: #f9f9f9;
    margin: 0;
    padding: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100vh;
}

/* Radio Group Container */
.radio-group {
    display: flex;
    flex-direction: column;
    gap: 10px;
    margin-bottom: 20px;
}

/* Label Styles */
/* Label Styles */
.radio-label {
    display: flex;
    align-items: center;
    font-size: 16px;
    color: #000; /* Letra negra */
    cursor: pointer;
    position: relative;
    padding: 10px 35px; /* Espaciado interno */
    background-color: #fff; /* Fondo blanco */
    border: 1px solid #ddd; /* Borde sutil */
    border-radius: 5px; /* Bordes redondeados */
    transition: background-color 0.3s ease, box-shadow 0.3s ease;
}

/* Custom Radio Button */
.custom-radio {
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    width: 20px;
    height: 20px;
    border: 2px solid #90c9a7;
    border-radius: 50%;
    background-color: #fff;
    transition: background-color 0.3s ease, box-shadow 0.3s ease;
}

/* Selected State */
.radio-label input[type="radio"]:checked + .custom-radio {
    background-color: #90c9a7;
    box-shadow: 0 0 0 4px rgba(144, 201, 167, 0.2);
}

/* Hide Native Radio */
.radio-label input[type="radio"] {
    position: absolute;
    opacity: 0;
    cursor: pointer;
}

/* Button Styles */
.btn-submit {
    background-color: #413d4b;
    color: #fff;
    padding: 10px 20px;
    font-size: 16px;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    transition: background-color 0.3s ease;
}

.btn-submit:hover {
    background-color: #90c9a7;
    color: #413d4b;
}
