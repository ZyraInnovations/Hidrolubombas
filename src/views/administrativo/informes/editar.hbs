<script>

document.addEventListener('DOMContentLoaded', async function () {
    // Obtener el valor del técnico seleccionado al cargar la página
    const tecnicoSelect = document.querySelector('select[name="tecnico"]');
    const tecnicoId = tecnicoSelect.value;

    // Si ya hay un técnico seleccionado al cargar la página, obtenemos su firma
    if (tecnicoId) {
        try {
            const response = await fetch(`/api/tecnico/firma/${tecnicoId}`);
            if (!response.ok) {
                throw new Error('Error al obtener la firma del técnico');
            }
            const data = await response.json();

            const firmaImg = document.getElementById('firma-tecnico');
            firmaImg.src = data.firma;
            firmaImg.style.display = 'block';
        } catch (error) {
            console.error('Error:', error.message);
            alert('No se pudo obtener la firma del técnico seleccionado.');
        }
    }
});

// Escuchar el cambio de técnico en el select
document.querySelector('select[name="tecnico"]').addEventListener('change', async function () {
    const tecnicoId = this.value;
    if (tecnicoId) {
        try {
            const response = await fetch(`/api/tecnico/firma/${tecnicoId}`);
            if (!response.ok) {
                throw new Error('Error al obtener la firma del técnico');
            }
            const data = await response.json();

            console.log(data.firma); // Asegúrate de que el formato sea correcto aquí

            const firmaImg = document.getElementById('firma-tecnico');
            firmaImg.src = data.firma;
            firmaImg.style.display = 'block';
        } catch (error) {
            console.error('Error:', error.message);
            alert('No se pudo obtener la firma del técnico seleccionado.');
        }
    } else {
        const firmaImg = document.getElementById('firma-tecnico');
        firmaImg.style.display = 'none';
    }
});


const textarea = document.querySelector("textarea[name='observaciones']");
textarea.style.height = `${textarea.scrollHeight}px`; // Ajusta automáticamente según el contenido

document.addEventListener('DOMContentLoaded', () => {
    const tecnicoSelect = document.querySelector('select[name="tecnico"]');
    const technicianPhoto = document.getElementById('technicianPhoto');

    // Función para cargar la foto del técnico
    const loadTechnicianPhoto = async (tecnicoId) => {
        if (tecnicoId) {
            try {
                // Realiza la solicitud a la API para obtener la foto del técnico
                const response = await fetch(`/api/tecnicos/${tecnicoId}`);
                if (response.ok) {
                    const blob = await response.blob();
                    // Crea una URL de objeto a partir del blob y establece el src de la imagen
                    technicianPhoto.src = URL.createObjectURL(blob);
                } else {
                    technicianPhoto.src = 'default-technician.jpg'; // Imagen por defecto en caso de error
                    console.error('Error al cargar la foto del técnico:', response.statusText);
                }
            } catch (error) {
                console.error('Error en la solicitud:', error);
            }
        } else {
            technicianPhoto.src = 'default-technician.jpg'; // Imagen por defecto cuando no hay técnico seleccionado
        }
    };

    // Verifica si ya hay un técnico seleccionado al cargar la página
    const selectedTecnicoId = tecnicoSelect.value;
    loadTechnicianPhoto(selectedTecnicoId);

    // Agrega un listener al cambio del selector
    tecnicoSelect.addEventListener('change', (event) => {
        const tecnicoId = event.target.value;
        loadTechnicianPhoto(tecnicoId);
    });
});











    // Cuando cambia el campo de entrada de archivo (cuando se seleccionan archivos), mostramos las imágenes
    document.getElementById('boton').addEventListener('change', function(event) {
        const files = event.target.files;
        const previewContainer = document.createElement('div');
        previewContainer.id = 'preview';
        document.body.appendChild(previewContainer);  // Añadimos el contenedor de vista previa al cuerpo

        // Limpiamos las vistas previas anteriores
        previewContainer.innerHTML = '';

        // Creamos una lista de archivos para gestionar las eliminaciones
        let fileList = Array.from(files); // Convertimos el FileList en un array

        // Recorremos los archivos y creamos vistas previas de imágenes
        fileList.forEach(function(file, index) {
            // Solo procesamos archivos de imagen
            if (file.type.startsWith('image/')) {
                const reader = new FileReader();
                
                reader.onload = function(e) {
                    const imgElement = document.createElement('img');
                    imgElement.src = e.target.result; // Establece la fuente de la imagen como el archivo

                    // Crear el contenedor para la imagen y el botón de eliminar
                    const imgWrapper = document.createElement('div');
                    imgWrapper.style.position = 'relative'; // Para posicionar la X sobre la imagen
                    imgWrapper.appendChild(imgElement);

                    // Crear el botón de eliminar (la X)
                    const removeButton = document.createElement('button');
                    removeButton.innerText = 'X';
                    removeButton.style.position = 'absolute';
                    removeButton.style.top = '5px';
                    removeButton.style.right = '5px';
                    removeButton.style.backgroundColor = 'red';
                    removeButton.style.color = 'white';
                    removeButton.style.border = 'none';
                    removeButton.style.borderRadius = '50%';
                    removeButton.style.width = '20px';
                    removeButton.style.height = '20px';
                    removeButton.style.fontSize = '12px';
                    removeButton.style.cursor = 'pointer';

                    // Cuando se haga clic en la X, eliminamos la imagen y el archivo
                    removeButton.addEventListener('click', function() {
                        imgWrapper.remove(); // Eliminar el contenedor de la imagen
                        fileList.splice(index, 1); // Eliminar el archivo de la lista

                        // Actualizar el input de archivo
                        const dataTransfer = new DataTransfer();
                        fileList.forEach(file => dataTransfer.items.add(file)); // Añadir archivos restantes
                        document.getElementById('boton').files = dataTransfer.files; // Actualizar el input
                    });

                    // Añadimos el botón de eliminar al contenedor
                    imgWrapper.appendChild(removeButton);

                    // Añadimos la imagen con la X al contenedor de vista previa
                    previewContainer.appendChild(imgWrapper);
                };

                reader.readAsDataURL(file); // Leemos el archivo de imagen como una URL de datos
            }
        });
    });
</script>


  
