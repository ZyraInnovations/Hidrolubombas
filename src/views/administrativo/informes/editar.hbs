async function fetchClienteInfo() {
    const clienteId = document.getElementById('clienteSelect').value;

    if (clienteId) {
        try {
            const response = await fetch(`/api/clientes/${clienteId}`);
            const data = await response.json();

            if (data.success || data.correo) {
                document.getElementById('correoInput').value = data.correo || 'No disponible';
                document.getElementById('clientPhoto').src = data.foto || 'default-client.jpg';
            } else {
                alert(data.message || 'No se pudo obtener la información del cliente.');
            }
        } catch (error) {
            console.error('Error al obtener la información del cliente:', error);
            alert('Hubo un error al intentar obtener la información del cliente.');
        }
    }
}

// También puedes agregar un evento al cambio del selector de cliente para actualizar la información cuando el usuario selecciona otro cliente
